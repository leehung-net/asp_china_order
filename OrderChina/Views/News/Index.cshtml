@{
    ViewBag.Title = "Index";
}
@using OrderChina.Models;
@model IEnumerable<OrderChina.Models.News>
<h2>Index</h2>
<div>
    
        @foreach (var sp in Model)
        {
            <div style="width:100%;height:600px">
                <table>
                    <tr>
                        <td rowspan="2" style="width:200px; height:100px"><img src="@sp.Img" style="width:200px; height: 100px" /></td>
                    </tr>
                    <tr>
                        <td style="width:400px;height:40px">@sp.Title</td>
                    </tr>
                    <tr>
                        <td>@sp.Titlebig</td>
                    </tr>
                </table>
            </div>
        }
</div>